#include <stdio.h>
int main()
{
    int a[100][100],i,j,m,n,k,sum=0;
    scanf("%d",&m);
    scanf("%d",&n);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        scanf("%d",&a[i][j]);
    }
    printf("enter column number:");
    scanf("%d",&k);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            if(j+1==k)
            {
                sum=sum+a[j][i];
            }
        }
    }printf("column size = %d",sum);
    return 0;
}
